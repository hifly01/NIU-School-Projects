         1 //KC003I4C JOB ,'CHARLES ALMS',MSGCLASS=H                               JOB01810
           //********************************************************************
           //* CSCI 465               ASSIGNMENT 5               FALL 2020      *
           //*                                                                  *
           //* DEVELOPER NAME: CHARELES ALMS                                    *
           //* DATE DUE: 10/19/2020                                             *
           //*                                                                  *
           //* Purpose: create a program to calculate the average and deposit/  *
           //* withdrawl amouunts from ochestra accounts that will be printed   *
           //* on a report. This assignment reads from 1 file and prints 2      *
           //* reports on the same output by opening and closing the files when *
           //* needed. File 2 is based off information of file 1                *
           //********************************************************************
           //*
           //********************************************************************
           //* JSTEP01 - the forst step that calls the COBOL compiler           *
           //*                                                                  *
           //* SYSIN - the input for the COBOL program that was provided to us  *
           //*                                                                  *
           //* SYSLIN - Output will be in a temp labeled &&TEMP                 *
           //*                                                                  *
           //* SYSPRINT - Output messages for the binder                        *
           //*                                                                  *
           //* SYSUT1-15 - Data pads/scratch work used by the compiler          *
           //*                                                                  *
           //* SYSUDECK - Part of the SYSUT1-15 for the COBOL compiler          *
           //*                                                                  *
           //* SYSDUMP -  Output messages for abends                            *
           //********************************************************************
         2 //JSTEP01 EXEC PGM=IGYCRCTL,PARM=APOST,REGION=0M
           //*
         3 //SYSIN   DD *
           //*
         4 //SYSLIN   DD DSN=&&TEMP,SPACE=(TRK,(5,10)),
           //            DISP=(MOD,PASS)
           //*
         5 //SYSPRINT DD SYSOUT=*
           //*
         6 //SYSUT1   DD SPACE=(CYL,(1,1))
         7 //SYSUT2   DD SPACE=(CYL,(1,1))
         8 //SYSUT3   DD SPACE=(CYL,(1,1))
         9 //SYSUT4   DD SPACE=(CYL,(1,1))
        10 //SYSUT5   DD SPACE=(CYL,(1,1))
        11 //SYSUT6   DD SPACE=(CYL,(1,1))
        12 //SYSUT7   DD SPACE=(CYL,(1,1))
        13 //SYSUT8   DD SPACE=(CYL,(1,1))
        14 //SYSUT9   DD SPACE=(CYL,(1,1))
        15 //SYSUT10  DD SPACE=(CYL,(1,1))
        16 //SYSUT11  DD SPACE=(CYL,(1,1))
        17 //SYSUT12  DD SPACE=(CYL,(1,1))
        18 //SYSUT13  DD SPACE=(CYL,(1,1))
        19 //SYSUT14  DD SPACE=(CYL,(1,1))
        20 //SYSUT15  DD SPACE=(CYL,(1,1))
        21 //SYSMDECK DD SPACE=(CYL,(1,1))
           //*
        22 //SYSUDUMP DD SYSOUT=*
           //*
           //*******************************************************************
           //* JSTEP02 - Executes the program 'HEWL' and will be the binder    *
           //* step. only works if the condition code is 0 meaning no abends   *
           //*                                                                 *
           //* SYSLIB - required for the compiler                              *
           //*                                                                 *
           //* SYSLIN - grabs the TEMP from JSTEP01                            *
           //*                                                                 *
           //* SYSLMOD - Load module/program object stored in PDSE             *
           //*                                                                 *
           //* SYSPRINT - Output of the registers for abend                    *
           //*******************************************************************
           //*
        23 //JSTEP02  EXEC PGM=HEWL,COND=(0,LT)
           //*
        24 //SYSLIB   DD DSN=CEE.SCEELKED,DISP=SHR
           //*
        25 //SYSLIN   DD DSN=&&TEMP,DISP=(OLD,DELETE,DELETE)
           //*
        26 //SYSLMOD  DD DSN=KC03I4C.CSCI465.LOADLIB(ENDOWMT2),
           //            SPACE=(1024,(50,20,1)),DSNTYPE=LIBRARY,
           //            DISP=(MOD,KEEP,KEEP)
           //*
        27 //SYSPRINT DD SYSOUT=*
           //*
           //*******************************************************************
           //* JSTEP03 - Fetches the program object/load module stored in my   *
           //* PDSE. Only works when less than 0, (no abends or errors)        *
           //*                                                                 *
           //* STEPLIN input will be used for accessing the data set from my   *
           //* loadlib                                                         *
           //*                                                                 *
           //* ENDFILE input will grab the temp object from setep one and use  *
           //* it for output                                                   *
           //*                                                                 *
           //* LEFILE saves space for the other report                         *
           //*                                                                 *
           //* RPTFILE output will print the data from                         *
           //* KC02322.CSCI465.DATAFA20(DATA5)                                 *
           //*                                                                 *
           //* SYSUDUMP output will provide a dump of registers and PSW        *
           //* incase of abend                                                 *
           //*******************************************************************
        28 //JSTEP03  EXEC PGM=ENDOWMT2,COND=(0,LT)
           //*
        29 //STEPLIB  DD DSN=KC03I4C.CSCI465.LOADLIB,DISP=SHR
           //*
        30 //ENDFILE  DD DSN=KC02322.CSCI465.DATAFA20(DATA5),
           //            DISP=SHR
           //*
        31 //LEFILE   DD SPACE=(TRK,(5,10)),DISP=(MOD,DELETE,DELETE)
           //*
        32 //RPTFILE  DD SYSOUT=*
           //*
        33 //SYSUDUMP DD SYSOUT=*
